{% extends "DestinyAppBundle::backendBase.html.twig" %}
{% block newButton %}
    {% if type != null %}
        {% set back =  entity ~'Contenido.list.back'  %}
        {% include('@DestinyApp/Backend/Include/newButton.html.twig' ) with {'element':section}%}
        <h2 class="sub-header">
            <a class="btn btn-success pull-right"
               href="{{ path('listBackend',{'entity':entity,}) }}">
                <i class="fa fa-chevron-circle-left"></i>
                {{ back | trans}}
            </a>
            <br>
        </h2>
    {% else %}
        {%  if group is not  iterable %}
            {{ parent() }}
        {% endif %}
    {% endif %}
{% endblock %}
{% block backendBody %}
    {% if group == true %}
    {% endif %}
    <div class="col-md-12">
        {% block listBody %}
            {% include('@DestinyApp/Backend/Include/listContent.html.twig' ) %}
        {% endblock %}
    </div>
{% endblock %}
